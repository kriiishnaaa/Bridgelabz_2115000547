{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {"type": "integer"},
      "name": {"type": "string"},
      "age": {"type": "integer"},
      "email": {"type": "string"},
      "skills": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    "required": ["id", "name", "email"]
  }
}
